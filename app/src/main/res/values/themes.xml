<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.NotOne" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/pink_900</item>
        <item name="colorPrimaryVariant">@color/purple_900</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_300</item>
        <item name="colorSecondaryVariant">@color/teal_900</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="colorAccent">@color/teal_300</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="colorCanvas">@color/white</item>
        <item name="colorTextDayNight">@color/black</item>
    </style>
</resources>